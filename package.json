{
  "description": "A Slackbot with the aim of getting people to exercise.",
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/log4js": "^2.3.5",
    "@types/node": "^15.12.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "coveralls": "3.1.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-xo": "0.36.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "^6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": "16.3.0"
  },
  "type": "module",
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "xo",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "files": [
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "target/index.js",
  "name": "fitbot",
  "scripts": {
    "clear-db": "rm data/database.*",
    "clean": "rm -rf target",
    "compile": "npm run clean && tsc",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node --require dotenv/config src/index.ts",
    "lint": "eslint '**/*.{js,ts}' --fix",
    "pretty": "prettier '**/*.json' --write",
    "test": "jest --verbose src",
    "test:watch": "jest --verbose --watch src",
    "start": "node --experimental-specifier-resolution=node --require dotenv/config target/index.js"
  },
  "version": "1.0.0",
  "dependencies": {
    "@slack/bolt": "^3.4.0",
    "@tsconfig/node16": "^1.0.1",
    "array-shuffle": "^2.0.0",
    "better-sqlite3-helper": "^3.1.3",
    "change-case": "^4.1.2",
    "dotenv": "^10.0.0",
    "eta": "^1.12.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "p-map": "^4.0.0",
    "p-reduce": "^3.0.0",
    "random": "^3.0.6",
    "uuid": "^8.3.2"
  }
}
