{
  "description": "A Slackbot with the aim of getting people to exercise.",
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/log4js": "^2.3.5",
    "@types/node": "^15.12.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "coveralls": "3.1.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-xo": "0.36.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": "16.1.0"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "xo",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "files": [
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "target/index.js",
  "name": "fitbot",
  "scripts": {
    "clear-db": "rm data/database.*",
    "build": "rm -rf target && tsc",
    "dev": "ts-node --require dotenv/config src/index.ts",
    "lint": "eslint '**/*.{js,ts}' --fix",
    "pretty": "prettier '**/*.json' --write",
    "test": "jest --verbose src",
    "test:watch": "jest --verbose --watch src",
    "start": "node --require dotenv/config target/index.js"
  },
  "version": "1.0.0",
  "dependencies": {
    "@slack/bolt": "^3.3.0",
    "array-shuffle": "^2.0.0",
    "better-sqlite3-helper": "^3.1.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "eta": "^1.12.1",
    "log4js": "^6.3.0",
    "random": "^3.0.6"
  }
}
